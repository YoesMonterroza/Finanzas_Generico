<Window x:Class="Finanzas_Generico.Vistas.InsertarLicencia"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Finanzas_Generico.Vistas"
        mc:Ignorable="d"
        Title="InsertarLicencia" Height="152" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="txtLicencia" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="188" Margin="84,25,0,0" />
        <Label x:Name="label" Content="Licencia" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="19,22,0,0" />
        <Button x:Name="btnAceptar" Content="Aceptar" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="54,75,0,0" IsDefault="True" Click="btnAceptar_Click" />
        <Button x:Name="btnCancelar" Content="Cancelar" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="160,75,0,0" IsCancel="True" Click="btnCancelar_Click" />
    </Grid>
</Window>